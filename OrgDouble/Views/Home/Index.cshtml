@model OrgDouble.Models.IdForBase
@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <h4>Добрый день. Необходимо ввести Id для поиска дубликатов</h4>

    <form asp-controller="Home" asp-action="Index">

        <div class="form-group">
            <lable asp-for="IdTo" class="control-label">Начальный Id</lable>
            <input type="number" asp-for="IdTo" />
        </div>
        <div class="form-group">
            <lable asp-for="IdFrom" class="control-label">Конечный Id</lable>
            <input type="number" asp-for="IdFrom" />
        </div>
        <div class="form-group">
            <input type="submit" value="Выбрать" class="btn btn-primary p-2 m-2" />
        </div>
    </form>




</div>

